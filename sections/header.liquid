<div class="navbar7_container">
    <a class="navbar7_logo-link w-nav-brand"
       href="{{ shop.secure_url }}">{{ shop.brand.logo | image_url: height: 125 | image_tag: class: "navbar7_logo" }}</a>
    <nav role="navigation" class="navbar7_menu w-nav-menu">
        <div class="navbar7_menu-left">
            {% assign menu_titles = "Opony,Felgi,Koła" | split: "," %}

            {% for menu_title in menu_titles %}
                <div class="navbar7_menu-dropdown w-dropdown" data-hover="true" data-delay="300">
                    <div class="navbar7_dropdown-toggle w-dropdown-toggle" aria-controls="w-dropdown-list-{{ forloop.index }}" aria-haspopup="menu" aria-expanded="false">
                        <div class="text-weight-semibold">{{ menu_title }}</div>
                        <div class="dropdown-chevron" style="transform: translate3d(5px, 10px, 0px) scale3d(1, 1, 1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) skew(0deg, 0deg); transform-style: preserve-3d;">
                            {% render 'select-arrow' %}
                        </div>
                    </div>
                    <nav class="navbar7_dropdown-list w-dropdown-list"
                         id="w-dropdown-list-{{ forloop.index }}"
                         aria-labelledby="w-dropdown-toggle-{{ forloop.index }}"
                         style="opacity: 1;">
                        <div class="navbar7_container-dropdown">
                            <div class="navbar7_dropdown-content">
                                {% case forloop.index %}
                                    {% when 1 %}
                                        <div class="navbar7_dropdown-content-left">
                                            {% assign vehicle_types = "Samochodowe,SUV,Motocyklowe,Ciężarowe,Dostawcze,Run Flat" | split: "," %}
                                            {% assign season_types = "Letnie,Zimowe,Całoroczne" | split: "," %}
                                            {% assign popular_sizes = "175/65 R14,185/65 R15,195/65 R15,205/55 R16,205/60 R16,225/45 R17" | split: "," %}
                                            {% assign tire_titles = "Opony według pojazdu,Opony Według Sezonu,Popularne Rozmiary Opon, Opony Według Producenta" | split: "," %}

                                            {% assign tire_classes = "Klasa Premium,Klasa Średnia,Klasa Ekonomiczna,Pozostałe" | split: "," %}
                                            {% assign premium_manufacturers = "Michelin,Continental,Bridgestone,Pirelli,Goodyear" | split: "," %}
                                            {% assign midrange_manufacturers = "Hankook,Yokohama,Falken,Kumho,Firestone" | split: "," %}
                                            {% assign economy_manufacturers = "Nankang,Kenda,Debica,Sava,Barum" | split: "," %}
                                            {% assign other_manufacturers = "Cooper,Tigar,Uniroyal,Fulda,General Tire" | split: "," %}
                                            {% assign tire_manufacturers = premium_manufacturers | concat: midrange_manufacturers | concat: economy_manufacturers | concat: other_manufacturers %}

                                            {% for tire_title in tire_titles %}
                                                <div class="navbar7_dropdown-link-list">
                                                    <h4 class="heading-style-h6">{{ tire_title }}</h4>
                                                    {% case forloop.index %}
                                                        {% when 1 %}
                                                            {% for vehicle_type in vehicle_types %}
                                                                <a href="#" class="navbar7_dropdown-link w-inline-block" tabindex="0">
                                                                    <div class="navbar7_icon-wrapper"></div>
                                                                    <div class="navbar7_item-text">
                                                                        <div>Opony {{ vehicle_type }}</div>
                                                                    </div>
                                                                </a>
                                                            {% endfor %}
                                                        {% when 2 %}
                                                            {% for season_type in season_types %}
                                                                <a href="#" class="navbar7_dropdown-link w-inline-block" tabindex="0">
                                                                    <div class="navbar7_icon-wrapper"></div>
                                                                    <div class="navbar7_item-text">
                                                                        <div>Opony {{ season_type }}</div>
                                                                    </div>
                                                                </a>
                                                            {% endfor %}
                                                        {% when 3 %}
                                                            {% for size in popular_sizes %}
                                                                <a href="#" class="navbar7_dropdown-link w-inline-block" tabindex="0">
                                                                    <div class="navbar7_item-text">
                                                                        <div>Opony {{ size }}</div>
                                                                    </div>
                                                                </a>
                                                            {% endfor %}
                                                        {% when 4 %}
                                                            {% for i in (0..tire_classes.size) %}
                                                                {% assign class = tire_classes[i] %}
                                                                {% assign start_index = i | times: 5 %}
                                                                {% assign manufacturers = tire_manufacturers | slice: start_index, 5 %}
                                                                <h4 class="heading-style-h6 text-size-regular">{{ class }}</h4>
                                                                {% for manufacturer in manufacturers %}
                                                                    <a href="/opony/{{ manufacturer | slugify }}" class="navbar7_dropdown-link w-inline-block" tabindex="0">
                                                                        <div class="navbar7_item-text">
                                                                            <div>{{ manufacturer }}</div>
                                                                        </div>
                                                                    </a>
                                                                {% endfor %}
                                                            {% endfor %}

                                                    {% endcase %}
                                                </div>
                                            {% endfor %}
                                        </div>
                                    {% when 2 %}
                                        <div class="navbar7_dropdown-content-rims">
                                            {% assign rims_titles = "Felgi Alumioniowe,Felgi Stalowe,Felgi Motocyklowe" | split: "," %}

                                            {% for rim_title in rims_titles %}
                                                {% assign href_slug = rim_title | slugify %}
                                                <a href="/{{ href_slug }}" class="link-block-2 w-inline-block" tabindex="0">
                                                    <div class="div-block-26">
                                                        {% comment %}
                                                        <img
                                                            src="https://assets-global.website-files.com/65945aac2ee770034538c7f4/65b3c68ab329b571e66b8e98_FelgiAluminiowe.webp"
                                                            loading="lazy" sizes="100vw"
                                                            srcset="https://assets-global.website-files.com/65945aac2ee770034538c7f4/65b3c68ab329b571e66b8e98_FelgiAluminiowe-p-500.webp 500w, https://assets-global.website-files.com/65945aac2ee770034538c7f4/65b3c68ab329b571e66b8e98_FelgiAluminiowe.webp 720w"
                                                            alt="" class="image-3">
                                                        {% endcomment %}
                                                    </div>
                                                    <div class="heading-style-h5 text-align-center">{{ rim_title }}</div>
                                                </a>
                                            {% endfor %}
                                        </div>
                                    {% when 3 %}
                                        {% comment %} Case for Wheels {% endcomment %}
                                {% endcase %}
                            </div>
                        </div>
                        <div class="navbar7_bottom-bar">
                            <div class="navbar7_bottom-bar-background-layer"></div>
                            <div class="navbar7_bottom-bar-left">
                                {% case forloop.index %}
                                    {% when 1 %}
                                        <p class="text-weight-semibold">Polecamy</p>
                                        <a href="#" class="text-style-link" tabindex="0">Artykuły</a>
                                        <a href="#" class="text-style-link" tabindex="0">Testy Opon</a>
                                        <a href="#" class="text-style-link" tabindex="0">Producenci Opon</a>
                                        <a href="#" class="text-style-link" tabindex="0">Dobór Opon Do Pojazdu</a>
                                    {% when 2 %}
                                        <a href="#" class="text-style-link" tabindex="0">Dobór Felg Do Pojazdu</a>
                                    {% when 3 %}
                                        <a href="#" class="text-style-link" tabindex="0">Dobór Kół Do Pojazdu</a>
                                {% endcase %}
                            </div>
                        </div>
                    </nav>
                </div>
            {% endfor %}
        </div>
        <form class="search"></form>
    </nav>
